#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/run-s/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/run-s/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/npm-run-all@4.1.5/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/run-s/index.js" "$@"
else
  exec node  "$basedir/../../bin/run-s/index.js" "$@"
fi
