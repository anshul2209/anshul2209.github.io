#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/@sveltejs+kit@2.43.8_@sveltejs+vite-plugin-svelte@6.2.1_svelte@4.2.20_vite@7.1.9_sass@1_68b34142d376eb3df568b084b99d96fc/node_modules/@sveltejs/kit/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/@sveltejs+kit@2.43.8_@sveltejs+vite-plugin-svelte@6.2.1_svelte@4.2.20_vite@7.1.9_sass@1_68b34142d376eb3df568b084b99d96fc/node_modules/@sveltejs/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/@sveltejs+kit@2.43.8_@sveltejs+vite-plugin-svelte@6.2.1_svelte@4.2.20_vite@7.1.9_sass@1_68b34142d376eb3df568b084b99d96fc/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/@sveltejs+kit@2.43.8_@sveltejs+vite-plugin-svelte@6.2.1_svelte@4.2.20_vite@7.1.9_sass@1_68b34142d376eb3df568b084b99d96fc/node_modules/@sveltejs/kit/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/@sveltejs+kit@2.43.8_@sveltejs+vite-plugin-svelte@6.2.1_svelte@4.2.20_vite@7.1.9_sass@1_68b34142d376eb3df568b084b99d96fc/node_modules/@sveltejs/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/@sveltejs+kit@2.43.8_@sveltejs+vite-plugin-svelte@6.2.1_svelte@4.2.20_vite@7.1.9_sass@1_68b34142d376eb3df568b084b99d96fc/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
