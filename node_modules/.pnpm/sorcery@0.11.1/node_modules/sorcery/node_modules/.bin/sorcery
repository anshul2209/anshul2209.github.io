#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/sorcery@0.11.1/node_modules/sorcery/bin/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/sorcery@0.11.1/node_modules/sorcery/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/sorcery@0.11.1/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/sorcery@0.11.1/node_modules/sorcery/bin/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/sorcery@0.11.1/node_modules/sorcery/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/sorcery@0.11.1/node_modules:/Users/anshul.bansal/Desktop/personal/anshul2209.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sorcery" "$@"
else
  exec node  "$basedir/../../bin/sorcery" "$@"
fi
